
@{
    ViewData["Title"] = "Cadastrar";
}

@model Cela

<h1>Cadastrar Cela</h1>

@TempData["msg"]

<form asp-action="cadastrar">

    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="AreaUtil"></label>
        <input asp-for="AreaUtil" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="QuantMaxima"></label>
        <input asp-for="QuantMaxima" class="form-control" />
    </div>
    <input type="submit" value="Cadastrar" class="btn btn-success"/>
</form>

