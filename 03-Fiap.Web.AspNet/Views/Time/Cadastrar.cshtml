
@{
    ViewData["Title"] = "Cadastrar";
}

@model Time
<h1>Cadastro</h1>


<form class="form" asp-action="Cadastrar" asp-controller="Time">
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input class="form-control" asp-for="Nome">
    </div>
    <div class="form-group">
        <label asp-for="DataFundacao"></label>
        <input class="form-control" asp-for="DataFundacao">
    </div>
    <div class="form-group">
        <label asp-for="FolhaPagamento"></label>
        <input class="form-control" asp-for="FolhaPagamento">
    </div>
    <div class="checkbox">
        <label asp-for="Classificacado"></label>
        <input type="checkbox" class="form-control" asp-for="Classificacado">
    </div>
    <div class="form-group">
        <label asp-for="Vitorias"></label>
        <input class="form-control" asp-for="Vitorias">
    </div>
    <input type="submit" value="OK" class="btn btn-primary">
    <a asp-controller="Time" asp-action="Listar" class="btn btn-primary">Listar</a>
</form>

