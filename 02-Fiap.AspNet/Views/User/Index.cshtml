@model _02_Fiap.AspNet.Models.Usuario


@{
    ViewData["Title"] = "Cadastrar";
}

<h1>Cadastro</h1>

<p>Nome: @ViewData["nome"]</p>
<p>Data: @ViewData["data"]</p>
<p>Email: @ViewBag.endEletronico</p>
<p>@TempData["msg"]</p>

<form action="/user/index" method="post">
    <div class="form-group">
        <label for="id-nome">Nome</label>
        <input type="text" name="nome" id="id-nome" class="form-control" />
    </div>
    <div class="form-group">
        <label for="id-data">Data Nascimento</label>
        <input type="text" name="dataNascimento" id="id-data" class="form-control" />
    </div>
    <div class="form-group">
        <label for="id-email">Email</label>
        <input type="text" name="email" id="email" class="form-control" />
    </div>
    <input type="submit" value="Cadastrar" class=" btn btn-primary" />
</form>

